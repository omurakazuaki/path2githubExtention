{
  "name": "path2github",
  "displayName": "Path2Github",
  "description": "Open the selected file on Github.",
  "version": "0.0.3",
  "publisher": "omurakazuaki",
  "engines": {
    "vscode": "^1.53.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:path2github.open",
    "onCommand:path2github.blame",
    "onCommand:path2github.openSelection",
    "onCommand:path2github.openWorkflow",
    "onCommand:path2github.newIssue"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "when": "filesExplorerFocus",
        "command": "path2github.open",
        "title": "Open on Github"
      },
      {
        "when": "resourceDirname =~ /.*..github.workflows$/ && resourceExtname == .yml",
        "command": "path2github.openWorkflow",
        "title": "Open Workflows on Github"
      },
      {
        "when": "filesExplorerFocus && !explorerResourceIsFolder",
        "command": "path2github.blame",
        "title": "Blame on Github"
      },
      {
        "when": "editorHasSelection || editorTextFocus",
        "command": "path2github.openSelection",
        "title": "Open on Github"
      },
      {
        "when": "editorHasSelection || editorTextFocus",
        "command": "path2github.newIssue",
        "title": "New Github issue"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "filesExplorerFocus",
          "command": "path2github.open",
          "group": "z_commands"
        },
        {
          "when": "resourceDirname =~ /.*..github.workflows$/ && resourceExtname == .yml",
          "command": "path2github.openWorkflow",
          "group": "z_commands"
        },
        {
          "when": "filesExplorerFocus && !explorerResourceIsFolder",
          "command": "path2github.blame",
          "group": "z_commands"
        }
      ],
      "editor/context": [
        {
          "when": "editorHasSelection || editorTextFocus",
          "command": "path2github.openSelection",
          "group": "z_commands"
        },
        {
          "when": "resourceDirname =~ /.*..github.workflows$/ && resourceExtname == .yml",
          "command": "path2github.openWorkflow",
          "group": "z_commands"
        },
        {
          "when": "editorHasSelection || editorTextFocus",
          "command": "path2github.newIssue",
          "group": "z_commands"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "publish": "vsce publish",
    "dev:package": "vsce package -o dist",
    "dev:install": "code --install-extension dist/path2github-0.0.2.vsix",
    "dev:deploy": "npm run dev:package && npm run dev:install",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
